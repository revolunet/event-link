{
  "name": "event-link",
  "version": "1.0.0",
  "description": "Generate calendar appointment URLs for popular calendar tools",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "calendar",
    "event",
    "google-calendar",
    "outlook",
    "ical"
  ],
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
